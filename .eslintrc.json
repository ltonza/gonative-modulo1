{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:react-native/all"],
  "env": { "jest": true },
  "plugins": ["react-native", "jsx-a11y", "import"],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "import/prefer-default-export": "off",
    "globals": {
      "__DEV__": true
    },
    "global-require": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "react/destructuring-assignment": "off"
  }
}
